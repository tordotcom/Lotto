
@{
    ViewBag.Title = "Admin";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <div class="card" id="ipContent">
            <div class="card-body">
                <center>IP Address ของคุณคือ <span id="ip"></span></center>
                <center>เข้าใช้งานล่าสุดเมื่อ <span id="lastSignout"></span> จาก IP Address <span id="lastIp"></span></center>
            </div>
        </div>
    </div>
</div>

<script>
    (function ($) {
        $.getJSON("http://jsonip.com?callback=?", function (data) {
            $("#ip").text(data.ip);
            $("#lastIp").text(data.ip);
        });
        $('#lastSignout').html('2019-01-01 13:00:00');
    })(jQuery);
</script>